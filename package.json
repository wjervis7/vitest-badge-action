{
  "name": "vitest-badge-action",
  "version": "0.0.0",
  "description": "A GitHub Action to parse vitest coverage results, and generate a badge",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "build:dev": "vite build --sourcemap",
    "format:fix": "prettier --write '**/*.ts'",
    "format": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "test": "vitest",
    "test-action": "node __tests__/main.tests.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://@github.com/wjervis7/vitest-badge-action.git"
  },
  "author": "wjervis7",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wjervis7/vitest-badge-action/issues"
  },
  "homepage": "https://github.com/wjervis7/vitest-badge-action#readme",
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "dependencies": {
    "@actions/core": "^1.10.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.2",
    "@tsconfig/node18": "^18.2.0",
    "@types/node": "^20.3.3",
    "@typescript-eslint/parser": "^5.60.1",
    "@vercel/ncc": "^0.36.1",
    "@vitest/coverage-istanbul": "^0.32.2",
    "@vitest/ui": "^0.32.2",
    "eslint": "^8.43.0",
    "eslint-plugin-github": "^4.8.0",
    "eslint-plugin-jest": "^27.2.1",
    "prettier": "^2.8.8",
    "typescript": "^5.1.6",
    "vite": "^4.3.9",
    "vitest": "^0.32.2"
  }
}
